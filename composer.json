{
	"name" : "luyadev/luya-legacy",
	"description" : "This repo contains mixed outdated content like blocks.",
	"type" : "luya-extension",
	"keywords" : ["luya"],
	"license": "MIT",
	"homepage" : "https://luya.io",
	"authors" : [
		{
			"name" : "Basil",
			"email" : "git@nadar.io",
			"homepage" : "https://github.com/nadar"
		}
	],
	"support" : {
		"issues" : "https://github.com/luyadev/luya-legacy/issues"
	},
	"require-dev" : {
		"luyadev/luya-testsuite" : "~1.0.0"
	},
	"autoload" : {
		"psr-4" : {
			"luya\\legacy\\" : "src/",
			"luya\\legacy\\tests\\" : "tests/"
		}
	},
	"extra": {
		"luya" : {
			"blocks" : [
				"src/blocks"
			]
		},
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "asset-pattern-skip-version": "(-build|-patch)",
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    }
}
